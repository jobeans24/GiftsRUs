<div class="events">
  {{#each events}}
    <article class="card">
      <div class="card-header">
        <h2>
          <a href="/events/{{id}}">{{title}}</a>
        </h2>
        <div>
          Posted by
          {{user.username}}
          on
          {{formatDate createdAt}}
        </div>
      </div>
      <div class="card-body">
        {{body}}
      </div>
    </article>
  {{/each}}
</div>